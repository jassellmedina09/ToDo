{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "User",
    "type": "object",
    "properties": {
      "uid": {
        "type": "string",
        "description": "Unique identifier for the user"
      },
      "display_name": {
        "type": "string",
        "description": "Display name of the user"
      },
      "email": {
        "type": "string",
        "format": "email",
        "description": "User's email address"
      },
      "birthdate": {
        "type": "string",
        "format": "date-time",
        "description": "User's birthdate stored as a timestamp"
      },
      "created_time": {
        "type": "string",
        "format": "date-time",
        "description": "Timestamp of when the user was created"
      },
      "photo_url": {
        "type": "string",
        "format": "uri",
        "description": "URL to the user's profile photo"
      },
      "hometown": {
        "type": "string",
        "description": "User's hometown"
      }
    },
    "required": ["uid", "display_name", "email", "birthdate", "created_time", "photo_url"],
    "additionalProperties": false
  }
  